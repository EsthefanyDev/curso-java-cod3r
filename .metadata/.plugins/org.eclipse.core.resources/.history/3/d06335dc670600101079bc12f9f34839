package fundamentos;

public class DesafioAritmeticos {
	public static void main(String[] args) {
		int a = 6;
		int b = 3;
		int c = 2;
		int d = 3;
		int e = 2;
		
		
		int result1 = b + c;
		System.out.println(result1);
		
		int result2 = result1 * b;
		System.out.println(result2);
		
		double result3 = Math.pow(result2, 3);
		System.out.println((int)result3);
		
		double result4 = result3 / (d * 3); 
		
		/*double resultSuperior1 = Math.pow(a * (b + c), 3);
